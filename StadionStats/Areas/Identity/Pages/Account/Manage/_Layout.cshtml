@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<header class="ex-header">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1>Rediger Konto</h1>
            </div>
        </div>
    </div>
</header>
<!-- Breadcrumb -->
<div class="breadcrumbs">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="text">
                    <a href="/">Forside</a><i class="fa fa-angle-double-right"></i><span>Kontoindstillinger</span>
                </div>
            </div>
        </div>
    </div>
</div>
<br /><br />
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-3">
                    <partial name="_ManageNav" />
                </div>
                <div class="col-md-9">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
</div>
<br /><br />

@section Scripts {
    @RenderSection("Scripts", required: false)
}
